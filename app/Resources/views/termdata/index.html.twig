{% extends 'base.html.twig' %}

{% block body %}
    <h1>TermData list</h1>

    <table>
        <thead>
            <tr>
                <th>Name</th>
                <th>Machinename</th>
                <th>Status</th>
                <th>Description</th>
                <th>Id</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for termDatum in termDatas %}
            <tr>
                <td><a href="{{ path('termdata_show', { 'id': termDatum.id }) }}">{{ termDatum.name }}</a></td>
                <td>{{ termDatum.machineName }}</td>
                <td>{{ termDatum.status }}</td>
                <td>{{ termDatum.description }}</td>
                <td>{{ termDatum.id }}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('termdata_show', { 'id': termDatum.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('termdata_edit', { 'id': termDatum.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('termdata_new') }}">Create a new entry</a>
        </li>
    </ul>
{% endblock %}
